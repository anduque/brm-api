{
  "name": "brm-api",
  "version": "1.0.0",
  "description": "API REST con Node.js, Express, Sequelize y PostgreSQL",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "docs": "apidoc -i src -o docs",
    "seed": "node src/config/seed.js",
    "db:sync": "node src/config/syncDatabase.js",
    "db:sync:force": "node src/config/syncDatabase.js --force",
    "db:sync:alter": "node src/config/syncDatabase.js --alter"
  },
  "keywords": [
    "api",
    "express",
    "sequelize",
    "postgresql"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "apidoc": "^1.2.0"
  }
}

